package devops.numbers;

public class NumberWords {
    
	public String toWords( int number ) {
		String returnValue = "";
		
	    if (number > 0)
	    {
	    //	returnValue	= String.valueOf(number) ;
	    	returnValue = "Inside Invalid Number  Loop";
	    	System.out.println("Inside correct Loop");
	    }
	    else{
	    	returnValue  = "Number out of range" ;
	    	System.out.println("Number is not out of range");
	    }	    	
		
		
		return returnValue;

	}

}
